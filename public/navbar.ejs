<button class="btn-notext rounded active" id="nav-ctrl">
	<svg
		xmlns="http://www.w3.org/2000/svg"
		width="25"
		height="25"
		class="bi bi-arrow-left-short"
		viewBox="0 0 16 16"
	>
		<path
			fill-rule="evenodd"
			d="M12 8a.5.5 0 0 1-.5.5H5.707l2.147 2.146a.5.5 0 0 1-.708.708l-3-3a.5.5 0 0 1 0-.708l3-3a.5.5 0 1 1 .708.708L5.707 7.5H11.5a.5.5 0 0 1 .5.5z"
		/>
	</svg>
</button>
<nav class="active">
	<div id="avatar" style="z-index: 2">
		<img src="./static/avatar-icon-png-12.png" alt="" />
	</div>
	<a href="/" class="<%= location === 'Home' ? 'title current' : 'title' %>"
		>Home</a
	>
	<a href="/work" class="<%= location === 'Work' ? 'title current' : 'title' %>"
		>Work</a
	>

	<% if (relLinks != undefined && relLinks.length > 0) { %> <% for( const {href,
	text} of relLinks ) { %>
	<a href="<%-href%>" class="rel-link"><%-text%> </a>
	<% } %> <% } %>
	<!-- <a class="rel-link active" href="/#intro">intro </a>
	<a class="rel-link" href="/#skills">skills 1</a>
	 -->
</nav>
